package klf.common.utils;

public class FanHuaState {

	public static String getFanHuaState(String code) {
		String state = null;
		
		switch (code) {
		case "2":
			state = "报价中";
			break;
		case "13":
			state = "报价失败";
			break;
		case "14":
			state = "报价成功";
			break;
		case "16":
			state = "核保中";
			break;
		case "19":
			state = "核保退回修改";
			break;
		case "20":
			state = "核保成功";
			break;
		case "23":
			state = "承保成功";
			break;
		case "25":
			state = "支付成功";
			break;
		case "30":
			state = "关闭";
			break;
		case "33":
			state = "完成";
			break;
		case "51":
			state = "承保政策限制";
			break;
		case "55":
			state = "报价退回修改";
			break;
		default:
			break;
		}
		
		return state;
	}
}
